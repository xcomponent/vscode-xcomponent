{
  "name": "component-viewer",
  "displayName": "component-viewer",
  "description": "A sample illustrating the use of TextContentProviders and the `vscode.previewHtml` command, introduce in 0.10.7",
  "version": "0.0.10",
  "license": "Apache-2.0",
  "publisher": "eg2",
  "galleryBanner": {
    "color": "#5c2d91",
    "theme": "dark"
  },
  "bugs": {
    "email": "egamma@microsoft.com"
  },
  "repository": {
    "type": "git"
  },
  "categories": [
    "Other"
  ],
  "engines": {
    "vscode": "^1.11.0"
  },
  "activationEvents": [
    "onLanguage:xml"
  ],
  "main": "out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.component-view",
        "title": "Component-view"
      }
    ]
  },
  "scripts": {
    "build": "npm run vscode:prepublish && webpack",
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "tslint": "tslint -c tslint.json src/extension.ts"
  },
  "dependencies": {
    "fs-extra": "^3.0.1",
    "gojs": "^1.7.9",
    "path": "^0.12.7",
    "vscode": "^1.1.0"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^3.0.0-beta.17",
    "babel-polyfill": "^6.23.0",
    "babel-runtime": "^6.20.0",
    "clean-webpack-plugin": "^0.1.16",
    "compression": "^1.6.2",
    "css-loader": "^0.26.1",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.24.1",
    "jasmine-reporters": "^2.2.0",
    "jest": "^17.0.3",
    "jest-cli": "^17.0.3",
    "jest-junit": "https://github.com/palmerj3/jest-junit.git",
    "node-sass": "^4.0.0",
    "sass-loader": "^4.1.0",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "ts-jest": "^17.0.3",
    "tslint": "^4.1.0",
    "tslint-loader": "^3.3.0",
    "typescript": "^2.1.4",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.2"
  }
}
