{
  "name": "xcomponent",
  "displayName": "XComponent",
  "description": "XComponent Viewer for Visual Studio Code",
  "version": "0.1.0",
  "publisher": "XComponent",
  "license": "Apache-2.0",
  "bugs": {
    "email": "dev@xcomponent.com",
    "url": "https://github.com/xcomponent/vscode-xcomponent/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xcomponent/vscode-xcomponent"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "xcomponent"
  ],
  "engines": {
    "vscode": "^1.11.0"
  },
  "activationEvents": [
    "*"
  ],
  "main": "out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "component-viewer",
        "title": "XCViewer: View cxml file"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "component-viewer"
        }
      ]
    }
  },
  "scripts": {
    "build": "npm run build:script && npm run build:extension",
    "build:script": "webpack -p",
    "build:extension": "tsc -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "tslint": "tslint -c tslint.json src/extension.ts",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "dependencies": {
    "@types/mocha": "2.2.39",
    "@types/node": "^7.0.5",
    "fs-extra": "^3.0.1",
    "gojs": "^1.7.9",
    "mocha": "^3.4.2",
    "path": "^0.12.7",
    "vscode": "^1.1.0",
    "xml2js": "^0.4.17"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^3.0.0-beta.17",
    "babel-polyfill": "^6.23.0",
    "clean-webpack-plugin": "^0.1.16",
    "compression": "^1.6.2",
    "css-loader": "^0.26.1",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.24.1",
    "jasmine-reporters": "^2.2.0",
    "jest": "^17.0.3",
    "jest-cli": "^17.0.3",
    "jest-junit": "https://github.com/palmerj3/jest-junit.git",
    "node-sass": "^4.0.0",
    "sass-loader": "^4.1.0",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "ts-jest": "^17.0.3",
    "tslint": "^4.1.0",
    "tslint-loader": "^3.3.0",
    "typescript": "^2.1.4",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.2"
  }
}
