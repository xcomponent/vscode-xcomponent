{
  "name": "xcomponent-viewer",
  "description": "XComponent Viewer",
  "version": "0.3.0",
  "license": "Apache-2.0",
  "scripts": {
    "compile:dev": "webpack -d --watch --progress",
    "compile": "webpack -p --define process.env.NODE_ENV='\"production\"' --progress --colors",
    "test": "jest --coverage ",
    "tslint": "tslint -c tslint.json src/**/*.ts*"
  },
  "author": "XComponent <dev@xcomponent.com> (http://www.xcomponent.com)",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/test/.*(test|spec))\\.tsx?$",
    "modulePaths": [
      "<rootDir>/node_modules/",
      "<rootDir>/src/"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/src/**/*.ts",
      "!**/src/**/*.d.ts"
    ],
    "setupTestFrameworkScriptFile": "./setup-jasmine-env.js",
    "unmockedModulePathPatterns": [
      "./node_modules/jasmine-reporters"
    ],
    "testEnvironment": "jsdom"
  },
  "dependencies": {
    "gojs": "^1.7.9"
  },
  "devDependencies": {
    "@types/expect": "^1.20.1",
    "@types/jest": "^19.2.4",
    "babel-polyfill": "^6.23.0",
    "clean-webpack-plugin": "^0.1.16",
    "compression": "^1.6.2",
    "css-loader": "^0.28.4",
    "expect": "^1.20.2",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.24.1",
    "jasmine-reporters": "^2.2.0",
    "jest": "^20.0.4",
    "jest-cli": "^20.0.4",
    "node-sass": "^4.0.0",
    "sass-loader": "^6.0.5",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "ts-jest": "^20.0.6",
    "ts-loader": "^2.1.0",
    "tslint": "^5.4.3",
    "tslint-loader": "^3.3.0",
    "typescript": "^2.3.4",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.2"
  },
  "false": {}
}
