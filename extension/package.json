{
  "name": "xcomponent",
  "displayName": "XComponent",
  "description": "XComponent Viewer for Visual Studio Code",
  "version": "0.4.0",
  "publisher": "XComponent",
  "license": "Apache-2.0",
  "icon": "images/logo_xc.png",
  "typings": "vscode.d.ts",
  "bugs": {
    "email": "dev@xcomponent.com",
    "url": "https://github.com/xcomponent/vscode-xcomponent/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xcomponent/vscode-xcomponent"
  },
  "homepage": "https://github.com/xcomponent/vscode-xcomponent/blob/master/extension/README.md",
  "categories": [
    "Other",
    "Snippets"
  ],
  "preview": true,
  "keywords": [
    "xcomponent"
  ],
  "engines": {
    "vscode": "^1.11.0"
  },
  "activationEvents": [
    "*"
  ],
  "main": "out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "xcomponent.preview.component",
        "title": "XComponent: Preview Component",
        "group": "navigation"
      },
      {
        "command": "xcomponent.preview.composition",
        "title": "XComponent: Preview Composition",
        "group": "navigation"
      },
      {
        "command": "xcomponent.preview.spy",
        "title": "XComponent: Launch Spy Web",
        "group": "navigation"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "xcomponent.preview.component"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "tslint": "tslint -c tslint.json src/**/*.ts*",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "dependencies": {
    "compression": "^1.7.0",
    "es6-promisify": "^5.0.0",
    "express": "^4.15.3",
    "port-pid": "^0.0.6",
    "portfinder": "^1.0.13",
    "portscanner": "^2.1.1",
    "vscode": "^1.1.2",
    "xml2js-parser": "^1.1.1"
  },
  "devDependencies": {
    "@types/chai": "^4.0.0",
    "@types/fs-extra": "^3.0.3",
    "@types/mocha": "^2.2.41",
    "@types/node": "^8.0.1",
    "chai": "^4.0.2",
    "install": "^0.10.1",
    "mocha": "^3.4.2",
    "ts-loader": "^2.1.0",
    "tslint": "^5.4.3",
    "tslint-loader": "^3.3.0",
    "typemoq": "^1.7.0",
    "typescript": "^2.3.4"
  },
  "false": {}
}
